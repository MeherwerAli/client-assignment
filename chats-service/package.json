{
  "name": "chat-service",
  "version": "1.0.0",
  "description": "NodeJS TypeScript Express Server",
  "main": "src/app.ts",
  "scripts": {
    "start": "nps",
    "serve": "npm start serve",
    "lint": "eslint -c eslintrc.json --ext .ts src",
    "test": "jest --coverage --no-cache",
    "coverage": "npm start test.unit.coverage",
    "build": "npm start build",
    "presetup": "npm install",
    "setup": "npm start config && npm start setup.script",
    "debug": "npm start serve",
    "lintfix": "eslint -c eslintrc.json --ext .ts src --fix",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "db:setup": "ts-node -e \"require('./test/utils/mongodb-setup').MongoDBSetup.initializeDatabase()\"",
    "db:cleanup": "ts-node -e \"require('./test/utils/mongodb-setup').MongoDBSetup.cleanupDatabase()\"",
    "db:check": "ts-node -e \"require('./test/utils/mongodb-setup').MongoDBSetup.checkConnection()\"",
    "test:e2e:setup": "npm run db:setup",
    "test:e2e:cleanup": "npm run db:cleanup",
    "test:full": "npm run test:e2e:setup && npm test && npm run test:e2e:cleanup"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express"
  ],
  "author": "Sayed M. Ali",
  "license": "ISC",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^7.8.0",
    "body-parser": "^1.20.2",
    "chalk": "^3.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "figlet": "^1.7.0",
    "http-status-codes": "^2.3.0",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongodb": "^5.8.1",
    "mongoose": "^8.3.4",
    "nodemon": "^3.1.0",
    "nps": "^5.10.0",
    "nps-utils": "^1.7.0",
    "openai": "^5.20.3",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typedi": "^0.8.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.14",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.3.0",
    "mongodb-memory-server": "^10.2.1",
    "prettier": "^3.3.2",
    "sinon": "^17.0.1",
    "ts-jest": "^29.1.2",
    "tslint": "^6.1.3",
    "typescript": "^5.4.5"
  }
}
